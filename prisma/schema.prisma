generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id      String   @id @default(uuid())
  account Account?

  @@map("users")
}

model Account {
  email    String @unique
  password String @unique

  user   User   @relation(fields: [userId], references: [id])
  userId String @unique

  @@map("accounts")
}
